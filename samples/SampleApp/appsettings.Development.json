{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "AspNetDebugDashboard": "Debug"
    }
  },
  "DebugDashboard": {
    "IsEnabled": true,
    "LogRequestBodies": true,
    "LogResponseBodies": false,
    "LogSqlQueries": true,
    "LogExceptions": true,
    "MaxEntries": 2000,
    "ExcludedPaths": ["/_debug", "/favicon.ico", "/robots.txt", "/swagger"],
    "ExcludedHeaders": ["Authorization", "Cookie", "X-API-Key"]
  }
}
