{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DebugDashboard": {
    "IsEnabled": true,
    "LogRequestBodies": true,
    "LogResponseBodies": true,
    "LogSqlQueries": true,
    "LogExceptions": true,
    "MaxEntries": 1000,
    "MaxBodySize": 1048576
  }
}
